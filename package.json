    "test:api-keys": "node -e \"import('./src/utils/loadEnv.js').then(m => { m.validateRequiredKeys(); console.log('✅ API Keys validation completed'); }).catch(e => { console.error('❌ API Keys error:', e.message); process.exit(1); })\"",
