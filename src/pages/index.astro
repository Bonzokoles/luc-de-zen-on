        testImageGeneration?: () => Promise<{ success: boolean; message: string; data?: any; }>;
        testAIBot?: () => Promise<{ success: boolean; message: string; data?: any; }>;
        testBigQuery?: () => Promise<{ success: boolean; message: string; data?: any; }>;
