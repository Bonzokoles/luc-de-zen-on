      z-index: 9999;
      padding: 20px;
      overflow-y: auto;
    }

    .wildcards-header {
      background: rgba(0, 0, 0, 0.55);
      padding: 20px;
      border: 1px solid rgba(255, 255, 255, 0.1);
      margin-bottom: 20px;
      display: flex;
      justify-content: space-between;
      align-items: center;
      flex-wrap: wrap;
    }

    .wildcards-header h1 {
      color: #00ffff;
      font-size: 2rem;
      font-weight: bold;
      margin: 0;
    }

    .wildcards-header p {
      color: #ffffff;
      margin: 0;
      opacity: 0.8;
    }

    .close-btn {
      background: rgba(255, 0, 0, 0.8);
      color: white;
      border: none;
      padding: 10px 20px;
      cursor: pointer;
      font-weight: bold;
      transition: background 0.3s;
    }

    .close-btn:hover {
      background: rgba(255, 0, 0, 1);
    }

    .wildcards-grid {
      display: grid;
      grid-template-columns: repeat(auto-fit, minmax(400px, 1fr));
      gap: 20px;
      max-width: 1400px;
      margin: 0 auto;
    }

    .wildcard-section {
      background: rgba(0, 0, 0, 0.55);
      border: 1px solid rgba(255, 255, 255, 0.1);
      padding: 20px;
    }

    .wildcard-section h2 {
      color: #00ffff;
      font-size: 1.2rem;
      margin-bottom: 15px;
      border-bottom: 1px solid rgba(0, 255, 255, 0.3);
      padding-bottom: 10px;
    }

    .wildcard-buttons {
      display: flex;
      flex-wrap: wrap;
      gap: 8px;
    }

    .wildcard-btn {
      background: rgba(0, 0, 0, 0.55);
      color: #ffffff;
      border: 1px solid rgba(255, 255, 255, 0.2);
      padding: 8px 16px;
      cursor: pointer;
      transition: all 0.3s ease;
      font-size: 0.9rem;
    }

    .wildcard-btn:hover {
      background: rgba(0, 255, 255, 0.2);
      border-color: #00ffff;
      color: #00ffff;
      transform: translateY(-2px);
    }

    .wildcard-btn:active {
      transform: translateY(0);
      background: rgba(0, 255, 255, 0.4);
    }

    .copy-status {
      position: fixed;
      top: 50%;
      left: 50%;
      transform: translate(-50%, -50%);
      background: rgba(0, 255, 0, 0.9);
      color: white;
      padding: 20px 40px;
      font-size: 1.2rem;
      font-weight: bold;
      z-index: 10000;
      transition: opacity 0.3s;
    }

    .copy-status.hidden {
      opacity: 0;
      pointer-events: none;
    }

    /* Responsive */
    @media (max-width: 768px) {
      .wildcards-grid {
        grid-template-columns: 1fr;
      }

      .wildcards-header {
        flex-direction: column;
        gap: 10px;
        text-align: center;
      }

      .wildcards-main-window {
        padding: 10px;
      }
    }
  </style>

