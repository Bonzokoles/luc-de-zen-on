<script>
let selectedProvider = null;
let selectedType = 'image';
let selectedStyle = 'realistic';
let generationHistory = [];

function selectProvider(provider, type) {
  selectedProvider = provider;
  selectedType = type;
  
  // Update UI
  document.querySelectorAll('.provider-card').forEach(card => {
    card.classList.remove('ring-4', 'ring-pink-400');
  });
  
  document.querySelector(`[data-provider="${provider}"]`).classList.add('ring-4', 'ring-pink-400');
  
  const providerNames = {
    'together': 'Together AI (FLUX.1 Schnell)',
    'stability': 'Stability AI (SDXL)',
    'huggingface': 'HuggingFace (Playground v2)',
    'openai': 'OpenAI Sora (Video)'
  };
  
  document.getElementById('current-provider').innerHTML = `
    <div class="text-lg">
      <span class="text-pink-400">Wybrano:</span> ${providerNames[provider]}
    </div>
    <div class="text-sm text-gray-400 mt-1">
      Typ: ${type === 'video' ? 'Film' : 'Obraz'} | Status: Darmowe ‚úÖ
    </div>
  `;
  
  // Enable generate button
  document.getElementById('generate-btn').disabled = false;
}

function selectStyle(style) {
  selectedStyle = style;
  
  document.querySelectorAll('.style-btn').forEach(btn => {
    btn.classList.remove('ring-2', 'ring-white');
  });
  
  document.querySelector(`[data-style="${style}"]`).classList.add('ring-2', 'ring-white');
}

async function generateContent() {
  const prompt = document.getElementById('prompt-input').value.trim();
  
  if (!prompt) {
    alert('Wprowad≈∫ opis tego co chcesz wygenerowaƒá!');
    return;
  }
  
  if (!selectedProvider) {
    alert('Wybierz dostawcƒô AI!');
    return;
  }
  
  // Show loading
  document.getElementById('loading').classList.remove('hidden');
  document.getElementById('results').classList.add('hidden');
  document.getElementById('generate-btn').disabled = true;
  
  try {
    const response = await fetch('/api/free-ai-models/image-video', {
      method: 'POST',
      headers: {
        'Content-Type': 'application/json'
      },
      body: JSON.stringify({
        prompt: prompt,
        provider: selectedProvider,
        type: selectedType,
        style: selectedStyle
      })
    });
    
    const data = await response.json();
    
    if (data.success) {
      displayResult(data);
      addToGallery(data);
    } else {
      throw new Error(data.error || 'B≈ÇƒÖd generowania');
    }
    
  } catch (error) {
    console.error('Generation error:', error);
    alert(`B≈ÇƒÖd generowania: ${error.message}`);
  } finally {
    // Hide loading
    document.getElementById('loading').classList.add('hidden');
    document.getElementById('generate-btn').disabled = false;
  }
}

function displayResult(data) {
  const resultContent = document.getElementById('result-content');
  
  if (data.type === 'video') {
    resultContent.innerHTML = `
      <video controls class="max-w-full mx-auto rounded-lg shadow-lg">
        <source src="${data.result.video_url}" type="video/mp4">
        Twoja przeglƒÖdarka nie obs≈Çuguje video.
      </video>
    `;
  } else {
    resultContent.innerHTML = `
      <img src="${data.result.image_url}" 
           alt="Generated image" 
           class="max-w-full mx-auto rounded-lg shadow-lg"
           onclick="openFullscreen(this)">
    `;
  }
  
  // Show generation info
  document.getElementById('generation-info').innerHTML = `
    <div class="grid grid-cols-1 md:grid-cols-3 gap-4 text-xs">
      <div><strong>Provider:</strong> ${data.provider}</div>
      <div><strong>Model:</strong> ${data.result.model}</div>
      <div><strong>Czas:</strong> ${new Date(data.timestamp).toLocaleString('pl-PL')}</div>
    </div>
    <div class="mt-2"><strong>Prompt:</strong> ${data.prompt}</div>
  `;
  
  document.getElementById('results').classList.remove('hidden');
}

function addToGallery(data) {
  generationHistory.unshift(data);
  
  const galleryGrid = document.getElementById('gallery-grid');
  
  const galleryItem = document.createElement('div');
  galleryItem.className = 'bg-white/10 backdrop-blur-md rounded-lg overflow-hidden';
  
  if (data.type === 'video') {
    galleryItem.innerHTML = `
      <video class="w-full h-48 object-cover" controls>
        <source src="${data.result.video_url}" type="video/mp4">
      </video>
      <div class="p-4">
        <p class="text-sm text-gray-300 truncate">${data.prompt}</p>
        <p class="text-xs text-pink-400 mt-1">${data.provider} ‚Ä¢ Video</p>
      </div>
    `;
  } else {
    galleryItem.innerHTML = `
      <img src="${data.result.image_url}" 
           alt="Generated" 
           class="w-full h-48 object-cover cursor-pointer"
           onclick="openFullscreen(this)">
      <div class="p-4">
        <p class="text-sm text-gray-300 truncate">${data.prompt}</p>
        <p class="text-xs text-pink-400 mt-1">${data.provider} ‚Ä¢ Obraz</p>
      </div>
    `;
  }
  
  galleryGrid.insertBefore(galleryItem, galleryGrid.firstChild);
  
  // Keep only last 12 items in gallery
  while (galleryGrid.children.length > 12) {
    galleryGrid.removeChild(galleryGrid.lastChild);
  }
}

function openFullscreen(img) {
  const modal = document.createElement('div');
  modal.className = 'fixed inset-0 bg-black/90 flex items-center justify-center z-50 p-4';
  modal.onclick = () => modal.remove();
  
  modal.innerHTML = `
    <img src="${img.src}" alt="Fullscreen" class="max-w-full max-h-full object-contain">
    <button class="absolute top-4 right-4 text-white text-3xl hover:text-gray-300" onclick="this.parentElement.remove()">√ó</button>
  `;
  
  document.body.appendChild(modal);
}

// Initialize
document.addEventListener('DOMContentLoaded', () => {
  // Add event listeners for provider cards
  document.querySelectorAll('.provider-card').forEach(card => {
    card.addEventListener('click', function() {
      const provider = this.dataset.provider;
      const type = this.dataset.type;
      selectProvider(provider, type);
    });
  });

  // Add event listeners for style buttons
  document.querySelectorAll('.style-btn').forEach(btn => {
    btn.addEventListener('click', function() {
      const style = this.dataset.style;
      selectStyle(style);
    });
  });

  // Add event listener for generate button
  document.getElementById('generate-btn').addEventListener('click', generateContent);

  // Select first style by default
  document.querySelector('.style-btn').classList.add('ring-2', 'ring-white');
  
  // Load API info
  fetch('/api/free-ai-models/image-video')
    .then(response => response.json())
    .then(data => {
      console.log('Free AI Models API loaded:', data);
    })
    .catch(error => {
      console.error('Failed to load API info:', error);
    });
});
</script>

