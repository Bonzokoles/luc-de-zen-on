{
  "id": "polaczek-workflow-t1-a1-d1",
  "name": "POLACZEK Tłumacz > Analityk > Dashboard",
  "description": "Przykład workflow designer w Flowise dla agentów POLACZEK (Tłumacz, Analityk, Dashboard).",
  "nodes": [
    {
      "id": "node-input",
      "type": "Input",
      "data": {
        "label": "User Input",
        "inputType": "text"
      },
      "position": { "x": 50, "y": 100 }
    },
    {
      "id": "node-t1",
      "type": "CustomREST",
      "data": {
        "label": "POLACZEK_T1 (Tłumacz)",
        "endpoint": "http://localhost:5005/agent/POLACZEK_T1",
        "method": "POST",
        "payloadMapping": { "text": "{{input}}" }
      },
      "position": { "x": 250, "y": 100 }
    },
    {
      "id": "node-a1",
      "type": "CustomREST",
      "data": {
        "label": "POLACZEK_A1 (Analityk)",
        "endpoint": "http://localhost:5005/agent/POLACZEK_A1",
        "method": "POST",
        "payloadMapping": { "analyze": true, "text": "{{node-t1.output.result}}" }
      },
      "position": { "x": 450, "y": 100 }
    },
    {
      "id": "node-d1",
      "type": "CustomREST",
      "data": {
        "label": "POLACZEK_D1 (Dashboard)",
        "endpoint": "http://localhost:5005/agent/POLACZEK_D1",
        "method": "POST",
        "payloadMapping": { "update": true, "analysis": "{{node-a1.output.result}}" }
      },
      "position": { "x": 650, "y": 100 }
    },
    {
      "id": "node-output",
      "type": "Output",
      "data": {
        "label": "Output",
        "outputType": "text",
        "outputMapping": { "dashboard": "{{node-d1.output.result}}" }
      },
      "position": { "x": 850, "y": 100 }
    }
  ],
  "edges": [
    { "id": "e-input-t1", "source": "node-input", "target": "node-t1" },
    { "id": "e-t1-a1", "source": "node-t1", "target": "node-a1" },
    { "id": "e-a1-d1", "source": "node-a1", "target": "node-d1" },
    { "id": "e-d1-output", "source": "node-d1", "target": "node-output" }
  ],
  "settings": {
    "trigger": "node-input",
    "responseNode": "node-output",
    "description": "Ten workflow uruchamia sekwencję: Tłumacz > Analityk > Dashboard. Każdy agent jest wywoływany przez REST API, wynik przechodzi dalej."
  }
}