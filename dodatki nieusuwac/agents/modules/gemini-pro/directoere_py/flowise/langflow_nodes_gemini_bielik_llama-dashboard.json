{
  "id": "polaczek-langflow-workflow",
  "name": "POLACZEK LangFlow: Gemini > Bielik > Llama > Dashboard",
  "description": "Przykład workflow LangFlow: wielomodelowa orkiestracja + dashboard.",
  "nodes": [
    {
      "id": "input-node",
      "type": "Input",
      "data": {
        "label": "User Prompt",
        "inputType": "text"
      },
      "position": { "x": 50, "y": 100 }
    },
    {
      "id": "gemini-node",
      "type": "LLMNode",
      "data": {
        "model": "gemini-pro",
        "endpoint": "http://localhost:9000/api/gemini",
        "params": { "temperature": 0.7, "max_tokens": 1024 },
        "input": "{{input-node}}"
      },
      "position": { "x": 250, "y": 80 }
    },
    {
      "id": "bielik-node",
      "type": "LLMNode",
      "data": {
        "model": "bielik",
        "endpoint": "http://localhost:11434/api/generate",
        "params": { "temperature": 0.5, "max_tokens": 1024 },
        "input": "{{gemini-node}}"
      },
      "position": { "x": 450, "y": 140 }
    },
    {
      "id": "llama-node",
      "type": "LLMNode",
      "data": {
        "model": "llama3",
        "endpoint": "http://localhost:11434/api/generate",
        "params": { "temperature": 0.3, "max_tokens": 1024 },
        "input": "{{bielik-node}}"
      },
      "position": { "x": 650, "y": 200 }
    },
    {
      "id": "dashboard-node",
      "type": "CustomRESTNode",
      "data": {
        "label": "Dashboard Live",
        "endpoint": "http://localhost:5005/agent/POLACZEK_D1",
        "method": "POST",
        "payload": { "ai_results": "{{llama-node}}" }
      },
      "position": { "x": 850, "y": 120 }
    },
    {
      "id": "output-node",
      "type": "Output",
      "data": {
        "label": "Final Output",
        "outputType": "text",
        "output": "{{dashboard-node}}"
      },
      "position": { "x": 1050, "y": 100 }
    }
  ],
  "edges": [
    { "id": "edge-input-gemini", "source": "input-node", "target": "gemini-node" },
    { "id": "edge-gemini-bielik", "source": "gemini-node", "target": "bielik-node" },
    { "id": "edge-bielik-llama", "source": "bielik-node", "target": "llama-node" },
    { "id": "edge-llama-dashboard", "source": "llama-node", "target": "dashboard-node" },
    { "id": "edge-dashboard-output", "source": "dashboard-node", "target": "output-node" }
  ],
  "settings": {
    "trigger": "input-node",
    "responseNode": "output-node",
    "description": "LangFlow: Input → Gemini → Bielik → Llama → Dashboard → Output"
  }
}