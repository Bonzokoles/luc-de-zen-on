{
  "id": "polaczek-langflow-workflow-extended",
  "name": "POLACZEK LangFlow: Vision, DoodleBart, PaLM, iCode Bizon, CodeBizon",
  "description": "LangFlow: Orkiestracja agentów AI, generacja kodu, dashboard live.",
  "nodes": [
    {
      "id": "input-node",
      "type": "Input",
      "data": { "label": "User Prompt", "inputType": "text" },
      "position": { "x": 50, "y": 100 }
    },
    {
      "id": "vision-node",
      "type": "LLMNode",
      "data": {
        "model": "gemini-vision",
        "endpoint": "http://localhost:9000/api/gemini-vision",
        "params": { "temperature": 0.5, "max_tokens": 512 },
        "input": "{{input-node}}"
      },
      "position": { "x": 250, "y": 80 }
    },
    {
      "id": "doodle-node",
      "type": "LLMNode",
      "data": {
        "model": "doodlebart",
        "endpoint": "http://localhost:9000/api/doodlebart",
        "params": { "temperature": 0.6, "max_tokens": 512 },
        "input": "{{input-node}}"
      },
      "position": { "x": 250, "y": 160 }
    },
    {
      "id": "palm-node",
      "type": "LLMNode",
      "data": {
        "model": "palm",
        "endpoint": "http://localhost:9000/api/palm",
        "params": { "temperature": 0.7, "max_tokens": 1024 },
        "input": "{{input-node}}"
      },
      "position": { "x": 450, "y": 80 }
    },
    {
      "id": "icode-node",
      "type": "LLMNode",
      "data": {
        "model": "icode-bizon",
        "endpoint": "http://localhost:9000/api/icode-bizon",
        "params": { "temperature": 0.3, "max_tokens": 1024 },
        "input": "{{palm-node}}"
      },
      "position": { "x": 650, "y": 80 }
    },
    {
      "id": "codebizon-node",
      "type": "LLMNode",
      "data": {
        "model": "codebizon",
        "endpoint": "http://localhost:9000/api/codebizon",
        "params": { "flow_type": "langflow", "max_tokens": 2048 },
        "input": "{{icode-node}}"
      },
      "position": { "x": 850, "y": 80 }
    },
    {
      "id": "dashboard-node",
      "type": "CustomRESTNode",
      "data": {
        "label": "Dashboard Live",
        "endpoint": "http://localhost:5005/agent/POLACZEK_D1",
        "method": "POST",
        "payload": {
          "vision": "{{vision-node}}",
          "doodle": "{{doodle-node}}",
          "codebizon": "{{codebizon-node}}"
        }
      },
      "position": { "x": 1050, "y": 100 }
    },
    {
      "id": "output-node",
      "type": "Output",
      "data": { "label": "Final Output", "outputType": "json", "output": "{{dashboard-node}}" },
      "position": { "x": 1250, "y": 100 }
    }
  ],
  "edges": [
    { "id": "edge-input-vision", "source": "input-node", "target": "vision-node" },
    { "id": "edge-input-doodle", "source": "input-node", "target": "doodle-node" },
    { "id": "edge-input-palm", "source": "input-node", "target": "palm-node" },
    { "id": "edge-palm-icode", "source": "palm-node", "target": "icode-node" },
    { "id": "edge-icode-codebizon", "source": "icode-node", "target": "codebizon-node" },
    { "id": "edge-vision-dashboard", "source": "vision-node", "target": "dashboard-node" },
    { "id": "edge-doodle-dashboard", "source": "doodle-node", "target": "dashboard-node" },
    { "id": "edge-codebizon-dashboard", "source": "codebizon-node", "target": "dashboard-node" },
    { "id": "edge-dashboard-output", "source": "dashboard-node", "target": "output-node" }
  ],
  "settings": {
    "trigger": "input-node",
    "responseNode": "output-node",
    "description": "LangFlow: Input → Vision/Doodle/PaLM → iCode → CodeBizon → Dashboard → Output. Dashboard node pozwala na integrację wyników agentów."
  }
}