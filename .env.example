# =======================================================
# KONFIGURACJA API KEYS - SYSTEM MYBONZO
# =======================================================

# OpenAI API Key (wymagany dla funkcji AI)
OPENAI_API_KEY=your-openai-api-key-here

# =======================================================
# ADMIN PANEL CONFIGURATION
# =======================================================
# Admin panel authentication
ADMIN_PASSWORD=HAOS77
ADMIN_SESSION_TIMEOUT=3600000

# Admin API security
ADMIN_API_KEY=your_admin_api_key_here
ADMIN_JWT_SECRET=your_jwt_secret_for_admin_tokens

# Default AI models for various functions
DEFAULT_TEXT_MODEL=@cf/meta/llama-3.1-8b-instruct
ADVANCED_TEXT_MODEL=@cf/qwen/qwen2.5-7b-instruct
IMAGE_MODEL=@cf/stabilityai/stable-diffusion-xl-base-1.0
AUDIO_MODEL=@cf/openai/whisper

# =======================================================
# FLOWISE AI CONFIGURATION
# =======================================================
PUBLIC_FLOWISE_API_URL=https://your-flowise-instance.com/api/v1/prediction/your-flow-id
PUBLIC_FLOWISE_API_TOKEN=your_flowise_api_token_here
FLOWISE_API_TOKEN=your_flowise_api_token_here

# =======================================================
# ACTIVEPIECES CONFIGURATION  
# =======================================================
PUBLIC_ACTIVEPIECES_API_URL=https://your-activepieces-instance.com/api/v1/flows/your-flow-id/run
PUBLIC_ACTIVEPIECES_API_TOKEN=your_activepieces_api_token_here
ACTIVEPIECES_API_KEY=your_activepieces_api_token_here

# =======================================================
# CLOUDFLARE CONFIGURATION
# =======================================================
CLOUDFLARE_AI_MODEL=@cf/meta/llama-3.1-8b-instruct
CLOUDFLARE_API_TOKEN=your-cloudflare-api-token-here
CLOUDFLARE_ACCOUNT_ID=your-cloudflare-account-id-here

# =======================================================
# GITHUB INTEGRATION (dla CI/CD)
# =======================================================
GITHUB_TOKEN=your-github-token-here

# =======================================================
# WEBHOOK SECURITY
# =======================================================
WEBHOOK_SECRET=your-webhook-secret-here

# =======================================================
# EMAIL & SMS NOTIFICATIONS
# =======================================================
EMAIL_API_KEY=your-email-service-api-key-here
SMS_API_KEY=your-sms-service-api-key-here

# =======================================================
# DATABASE & STORAGE
# =======================================================
DATABASE_URL=your-database-connection-string-here
STORAGE_API_KEY=your-storage-service-api-key-here

# =======================================================
# SITE CONFIGURATION
# =======================================================
PUBLIC_SITE_URL=https://www.mybonzo.com

# =======================================================
# ADDITIONAL INTEGRATIONS
# =======================================================
# Tavily Search API (jeśli używane)
TAVILY_API_KEY=your-tavily-api-key-here

# BigQuery Integration (jeśli używane)
GOOGLE_CLOUD_PROJECT_ID=your-gcp-project-id
GOOGLE_CLOUD_PRIVATE_KEY=your-gcp-private-key

# Kaggle API (jeśli używane)
KAGGLE_USERNAME=your-kaggle-username
KAGGLE_KEY=your-kaggle-api-key

# =======================================================
# DEVELOPMENT/DEBUGGING
# =======================================================
# Ustaw na 'development' dla trybu deweloperskiego
NODE_ENV=production

# Ustaw na 'true' żeby włączyć szczegółowe logi
DEBUG_MODE=false

# Public WebSocket endpoint for local development of POLACZEK assistant
# Example for wrangler dev default port
PUBLIC_WS_URL=ws://127.0.0.1:8787/ws/polaczek

# =======================================================
# ADMIN PANEL ADVANCED CONFIGURATION
# =======================================================
# Monitoring & Analytics
MONITORING_ENABLED=true
ANALYTICS_TRACKING_ID=your_analytics_id
ERROR_REPORTING_ENDPOINT=your_error_reporting_url

# Performance monitoring
PERFORMANCE_MONITORING=true
UPTIME_CHECK_INTERVAL=30000
HEALTH_CHECK_TIMEOUT=5000

# Security settings
CORS_ALLOWED_ORIGINS=http://localhost:4321,https://your-domain.com
RATE_LIMIT_REQUESTS_PER_MINUTE=100
RATE_LIMIT_BURST=20

# Logging
LOG_LEVEL=info
LOG_RETENTION_DAYS=30
AUDIT_LOG_ENABLED=true
AUDIT_LOG_RETENTION_DAYS=90

# Feature flags
FEATURE_VOICE_AI=true
FEATURE_IMAGE_GENERATION=true
FEATURE_ADVANCED_ANALYTICS=true
FEATURE_USER_MANAGEMENT=true
FEATURE_AUDIT_LOGS=true

# Blade Runner quotes system
BLADE_RUNNER_QUOTES_ENABLED=true
QUOTE_ROTATION_INTERVAL=300000

# Cyber theme configuration
CYBER_THEME_ENABLED=true
NEO_EFFECTS_ENABLED=true
MATRIX_BACKGROUND=false
