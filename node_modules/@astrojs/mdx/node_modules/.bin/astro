#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/q/mybonzo/luc-de-zen-on/node_modules/.pnpm/astro@5.13.5_jiti@2.5.1_lig_ec3e05e78fc7e7ec26fbade884aabc3b/node_modules/astro/node_modules:/mnt/q/mybonzo/luc-de-zen-on/node_modules/.pnpm/astro@5.13.5_jiti@2.5.1_lig_ec3e05e78fc7e7ec26fbade884aabc3b/node_modules:/mnt/q/mybonzo/luc-de-zen-on/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/q/mybonzo/luc-de-zen-on/node_modules/.pnpm/astro@5.13.5_jiti@2.5.1_lig_ec3e05e78fc7e7ec26fbade884aabc3b/node_modules/astro/node_modules:/mnt/q/mybonzo/luc-de-zen-on/node_modules/.pnpm/astro@5.13.5_jiti@2.5.1_lig_ec3e05e78fc7e7ec26fbade884aabc3b/node_modules:/mnt/q/mybonzo/luc-de-zen-on/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../astro@5.13.5_jiti@2.5.1_lig_ec3e05e78fc7e7ec26fbade884aabc3b/node_modules/astro/astro.js" "$@"
else
  exec node  "$basedir/../../../../../../astro@5.13.5_jiti@2.5.1_lig_ec3e05e78fc7e7ec26fbade884aabc3b/node_modules/astro/astro.js" "$@"
fi
